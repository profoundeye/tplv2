#feedArea .content .video_windows embed { margin:0 0 10px; width:855px; }

.btn_bottom { position:fixed; right:1px; bottom:-3px; _right:expression(eval(document.documentElement.scrollLeft+1+"px")); _bottom:expression(eval(document.documentElement.scrollTop+-3+"px")); z-index:1000; padding:0; width:510px; }

.btn_bottom .shoot_title { height:30px; background:url(../image/nav_bg.png) repeat-x 0 0; border:1px #aaa solid;  line-height:30px; padding:0 10px; float:left; margin:0 0 10px; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; text-shadow:0px 1px 0px #fff; color:#666; font-weight:700; max-width:480px; overflow:hidden; }
.btn_bottom .shoot_close { float:left; width:25px; height:25px; margin:3px 0 0 10px; }
