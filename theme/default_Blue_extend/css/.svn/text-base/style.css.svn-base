@charset "utf-8";

body { background:url(../image/background.gif) repeat 0 0;position:relative; } /*页面背景色*/

a:link, a:visited {color:#666;text-decoration: none;} /*默认超链接*/
a:hover {color:#333;text-decoration: none;}

#header_bg {  background:url(../image/topbg.png) no-repeat 50% 0%;margin:0 auto; width:100%; padding:0; } 

#wrap { width:900px; margin:0 auto; padding:0; }
#xia {z-index:10;position: fixed;bottom:0px; background:url(../image/xia.png) repeat 0 0;width:98%;min-width:930px;height:30px;border-radius: 5px 5px 0 0;margin-left:1%;border:1px #ccc solid;border-width:1px 1px 0 1px;}
#top {float: left;position:relative;width:900px;height:220px;}
#main { width:900px; float:left; position:relative; margin:-20px 0 0; }
.none { display:none; }

#feed_loading {z-index:-1;position: absolute;right:100px; bottom:35px; width:100px; height:26px; margin:0; background:url(../image/feeds_loading.gif) no-repeat 0 0; margin-left:265px; }

.backToTop{right:10px;bottom: 35px;}
/*头像*/
#avatar { position:absolute; bottom: 5px; left:20px; }
#avatar .face { -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px; overflow:hidden; }
#avatar a .head_bg{ width:65px; height:65px; padding:3px; background:url(../image/avatar_bg.png) repeat-x 0 0; border:1px #227CAC solid; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; -moz-box-shadow:0px 1px 3px #999; -webkit-box-shadow:0px 1px 3px #999; box-shadow:0px 1px 3px #999; }
#avatar a:hover .head_bg { background:url(../image/avatar_bg.png) repeat-x 0 -75px; border:1px #C64444 solid; }

/*博客名称*/
#usertitle { width:98% ; height:35px; overflow:hidden; }
#main .blogname {  position: absolute;left:50px;top:40px;color:#666; }
#main .blogname a { float:left; font-size:30px; font-weight:700; line-height:70px; text-shadow:0px 1px 0px #fff; height:70px; }
#main .blogname a:hover { color:#333; text-shadow:0px 1px 3px #999; }

/*关注状态 - 未关注*/
#usertitle .btn_follow_0 a {line-height:30px; height:28px; padding:0 10px; background:url(../image/blog_btn_bg.png) repeat-x 0 0; border:1px #61A701 solid; color:#fff; text-shadow:0px 1px 0px #61A701; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; margin: 0 0 20px 100px;}
#usertitle .btn_follow_0 a:hover { background-image:none; background-color:#7DDB00; }
#usertitle .btn_follow_0 a:active { -moz-box-shadow:0px 1px 3px #61A701 inset; -webkit-box-shadow:0px 1px 3px #61A701 inset; box-shadow:0px 1px 3px #61A701 inset; }

/*关注状态 - 已关注*/
#usertitle .btn_follow_1 a {line-height:30px; height:28px; padding:0 10px; background:url(../image/blog_btn_bg.png) repeat-x 0 -50px; border:1px #ccc solid; color:#999; text-shadow:0px 1px 0px #fff; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; margin: 0 0 20px 100px;}
#usertitle .btn_follow_1 a:hover { background:url(../image/avatar_bg.png) repeat-x 0 -75px; border:1px #C64444 solid; color:#fff; text-shadow:0px 1px 0px #C64444; }

/*博客个性签名*/
.userinfo { position: absolute;left:30px;top:90px;width:400px; margin:0 10px;  padding:10px 15px; line-height:22px;  text-shadow:0px 1px 0px #fff;}
.userinfo .quote { background:url(../image/blockquote.png) no-repeat left top; width:400px; margin:0; }
.userinfo blockquote { background:url(../image/blockquote-over.png) no-repeat bottom right; text-indent:2em; display:block; width:100%; }

/*博客关注标签*/
#mytag { position: absolute;left:10px;bottom:-25px;width:630px; margin:10px;z-index:1; }
#mytag li { list-style:none; outline:none; display:block; float:left;  margin:0 5px 5px 0; }
#mytag li a { border:1px #ccc solid; display:block; background:url(../image/header_bg.png) repeat-x 0 -30px; line-height:23px; height:23px; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px; -moz-box-shadow:0px 1px 0px #fff; -webkit-box-shadow:0px 1px 0px #fff; box-shadow:0px 1px 0px #fff; font-size:12px; color:#666; text-shadow:0px 1px 0px #fff; }
#mytag li a:hover { background-color:#e3e3e3; background-image:none; }
#mytag li a span { background:url(../image/blog_icon.png) no-repeat 0 0; padding:0 10px 0 25px; line-height:23px; height:23px; display:block; }

/*博客内容列表*/
#feed_box { width:630px; margin:10px; padding:0; }
#feed_box .box { padding-bottom: 27px;float:left; width:900px; margin:10px 0 0;  min-height:150px; position:relative;border-bottom:2px #ddd solid\9; }
#feed_box .date {z-index:1; width:90px; height:90px; background-color:#ccc; -moz-border-radius:8px; -webkit-border-radius:8px; border-radius:8px;  -moz-box-shadow:0px 2px 3px #ddd; -webkit-box-shadow:0px 2px 3px #ddd; box-shadow:0px 2px 3px #ddd; position:absolute; right:35px; top:-28px;  }
#feed_box .date .month { width:88px; height:23px; line-height:23px; border:1px #37B6FF solid; background:url(../image/avatar_bg.png) repeat-x 0 0px; -moz-border-radius:8px 8px 0 0; -webkit-border-radius:8px 8px 0 0; border-radius:8px 8px 0 0; text-align:center; color:#fff; text-shadow:0px -1px 0px #37B6FF; }
#feed_box .date .day { width:88px; height:64px; margin:0 1px 1px; background-color:#fff; -moz-border-radius:0 0 7px 7px; -webkit-border-radius:0 0 7px 7px; border-radius:0 0 7px 7px; line-height:64px; text-align:center; font-size:36px; color:#666; font-weight:700; }
#feed_box .title { background-position:0px 0px;width:894px; min-height:33px;padding-top: 10px; margin:0; font-size:20px; text-align:left; }
#feed_box .title a{margin-left:25px;color: #fff}
#feed_box .title a:hover{color: #c5eaff}

#feed_box .content { float: left;background-position:-1845px 0px;background-repeat:repeat-y;width:855px; min-height:100px; position:relative; font-size:14px; color:#666; padding:15px 20px; }
#feed_box .content p { margin:0; line-height:25px; float:left; width:860px; }
#feed_box .content p a { text-decoration:underline; margin:0 5px; }
#feed_box .content a.readMore { font-size:12px; line-height:25px; color:#999; display:block; }

/*发布功能区*/
#feed_box .footer { float: left;background-position:-2782px 100%;width:846px; line-height:25px; height:40px; bottom:10px; left:15px;padding:0 25px; }
#feed_box .footer .tag { background:url(../image/icons.png) no-repeat -102px -110px;width:300px; float:left; overflow:hidden; height:25px;padding-left: 23px; }
#feed_box .footer .tag a { margin-right:5px; }

#feed_box .more{float: left;background-position: -1845px 0px;background-repeat: repeat-y;font-size: 14px;padding:5px 20px;width: 855px;}

#feed_box .footer .fun { float:right; }
#feed_box .footer .fun a { float:right; height:23px; margin:0 0 0 5px; outline:none; }
#feed_box .footer .fun a em { float: right;font-style:normal; display:block;  color:#ccc;font-size:12px; }
#feed_box .footer .fun a:hover em { background-color:#DF584E; }
#feed_box .footer .fun .hotnum a { min-width:45px;padding-left: 23px; background:url(../image/icons.png) no-repeat -1px -491px;  }
#feed_box .footer .fun .hotnum a:hover { background:url(../image/icons.png) no-repeat -1px -516px; }
#feed_box .footer .fun .reply a { min-width:33px;padding-left: 23px; background:url(../image/icons.png) no-repeat -1px -441px;  }
#feed_box .footer .fun .reply a:hover { background:url(../image/icons.png) no-repeat -1px -466px; }
#feed_box .footer .fun .nolike a { width:33px;padding-left: 23px; background:url(../image/icons.png) no-repeat -101px -491px; }
#feed_box .footer .fun .nolike a:hover { background:url(../image/icons.png) no-repeat -101px -516px; }
#feed_box .footer .fun .like a { width:33px;padding-left: 23px; background:url(../image/icons.png) no-repeat -101px -541px; }
#feed_box .footer .fun .reblog a { width:33px;padding-left: 23px; background:url(../image/icons.png) no-repeat -101px -566px; }
#feed_box .footer .fun .reblog a:hover { background:url(../image/icons.png) no-repeat -101px -591px; }
#feed_box .footer .fun .delete a { width:23px; background:url(../image/icons.png) no-repeat -101px -441px; }
#feed_box .footer .fun .delete a:hover { background:url(../image/icons.png) no-repeat -101px -466px; }
#feed_box .footer .fun .edit a { width:33px;padding-left: 23px; background:url(../image/icons.png) no-repeat -1px -541px; }
#feed_box .footer .fun .edit a:hover { background:url(../image/icons.png) no-repeat -1px -566px;  }
#feed_box .footer .fun .view a { width:23px; background:url(../image/icons.png) no-repeat -1px -591px; }
#feed_box .footer .fun .view a:hover { background:url(../image/icons.png) no-repeat -101px -591px;  }

/*博客文字模型*/
#feed_box .content .text_area img { max-width:245px; float:left; margin-bottom:5px; margin-right:10px; }
#feed_box .content .text_area, #feedArea .content .text_area_all { width:100%; margin:0; padding:0; }
#feed_box .content .text_area p { margin:5px 0 0 5px;  }
#feed_box .content .text_area_all p { margin:5px 0 0 5px; }
#feed_box .content .text_area_all img { max-width:240px; margin-bottom:5px; margin-right:5px;}
#feed_box .content a:hover img { filter:alpha(opacity=70); -moz-opacity:0.7; opacity: 0.7; -webkit-transition:all 0.3s ease-in; -moz-transition:all 0.3s ease-in; transition:all 0.3s ease-in;  }

/*博客图片模型*/
#feedArea .content canvas { max-width:500px; margin:5px 5px 1px 5px; border:none; #margin:5px;}
#feedArea .content .img_top img { max-width:410px; margin:0px; border:none; #margin:5px;background-color: #f9f9f9; border:1px solid #ccc;padding:5px;}
#feedArea .content .img_top li { float: left;list-style:none; display:block; position:relative; padding:0; margin:0;}
#feedArea .content .img_k .img_two img { height:150px; padding:5px; border:none;background-color: #f9f9f9; border:1px solid #ccc  }
#feedArea .content .img_k .img_two li { list-style:none;display:block; float:left; margin:0px; overflow:hidden; }
#feedArea .content .img_k .img_three img { height:150px; padding:5px; border:none;background-color: #f9f9f9; border:1px solid #ccc  }
#feedArea .content .img_k .img_three li { list-style:none;display:block; float:left; margin:0px; overflow:hidden;}
#feedArea .content .img_k .img_two{}
#feedArea .content .img_k .img_three{}
.img_k_bimg .img_two{float: left;width:410px;}
.img_k_bimg .img_three{float: right;width:410px;margin-right:15px}
/*博客图片模型 - 图片控制*/
.image_ctrl { position:absolute; z-index:99;height:26px;margin-top:8px;display:none;}
.image_ctrl a span { width:26px; height:26px; display:block; float:right; margin:0 0 0 5px; text-indent:-9999em; cursor:pointer; }

.image_ctrl a .imgreal { background:url(../image/icons_none.png) no-repeat -60px -50px; }
.image_ctrl a:hover .imgreal { background:url(../image/icons_none.png) no-repeat -60px -80px; }
.image_ctrl a .clockright { background:url(../image/icons_none.png) no-repeat -30px -50px; }
.image_ctrl a:hover .clockright { background:url(../image/icons_none.png) no-repeat -30px -80px; }
.image_ctrl a .clockleft { background:url(../image/icons_none.png) no-repeat 0 -50px; }
.image_ctrl a:hover .clockleft { background:url(../image/icons_none.png) no-repeat 0 -80px; }

/*博客音乐模型*/
#feed_box .content .album { width:100%; height:102px; margin:0 0 10px; }
#feed_box .content .album .cover { width:140px; height:102px; float:left; position:relative; }
#feed_box .content .album .cover img { width:100px; height:100px; border:1px #666 solid; margin:0; float:left; }
#feed_box .content .album .cover_img { width:140px; height:100px; background:url(../image/album.png) no-repeat 0 0; position:absolute; left:1px; top:1px; }
#feed_box .content .album .cover_title { width:360px; height:30px; line-height:30px; padding-left:10px; margin:0; overflow:hidden; }
#feed_box .content .album .cover_title a { color:#666; }
#feed_box .content .album .cover_title a:hover { color:#333; }

#feed_box .content .album .cover_fun { width:360px; margin-left:10px; height:30px; line-height:30px; }
#feed_box .content .album .cover_fun a span { font-size:12px; color:#999; margin-left:10px; }
#feed_box .content .album .cover_fun a:hover span { text-decoration:underline; }
#feed_box .content .album .cover_fun a .eject { background:url(../image/icons.png) no-repeat 0 -620px; padding-left:13px; }
#feed_box .content .album .cover_fun a .download { background:url(../image/icons.png) no-repeat -70px -620px; padding-left:13px; }
#feed_box .content .album embed { margin:9px auto 0 10px;}
#feed_box .content .album object { margin:9px auto 0 10px\9; #margin:9px auto 0 10px; }

/*视频控制*/
.w240 { width:240px; margin:0 7px 15px; }
.mw240 { max-width:240px; margin:0 0 15px; }
.w510 { width:510px; margin:0 0 15px; }

/*发布音乐视频*/
#feed_box .content .video { padding:0; float:left; overflow:hidden; position:relative; }
#feed_box .content .video .video_bg { width:100%; margin:0; padding:0; float:left; }

/*发布音乐视频 - 音悦台*/
#feedArea .content .video .video_bg img { width:120px; -moz-border-radius:4px; -webkit-border-radius:4px; border-radius:4px; margin:0; padding:0; float:left; border:1px #666 solid; }
#feedArea .content .video a .video_play { width:30px; height:30px; display:none; background:url(../image/icons.png) no-repeat 0 -640px; position:absolute; z-index:1; left:5px; bottom:5px; }
#feedArea .content .video a:hover .video_play { display:block; }
#feedArea .content .video .video_song { position:absolute; left:122px; top:5px; height:30px; line-height:30px; padding:0 5px; max-width:118px; background-color:#4BB2EA; color:#fff; overflow:hidden; font-size:14px; -moz-border-radius:0px 5px 5px 0px; -webkit-border-radius:0px 5px 5px 0px; border-radius:0px 5px 5px 0px; }
#feedArea .content .video .video_name { position:absolute; left:122px; top:40px; height:25px; line-height:25px; padding:0 5px; background-color:#F98080; color:#fff; max-width:118px; overflow:hidden;  font-size:12px; -moz-border-radius:0px 5px 5px 0px; -webkit-border-radius:0px 5px 5px 0px; border-radius:0px 5px 5px 0px; }

#feedArea .content .video .video_title{ position:absolute; left:122px; top:5px; line-height:25px; max-height:75px; padding:0 5px; max-width:118px; background-color:#aaa; color:#fff; overflow:hidden; font-size:12px; -moz-border-radius:0px 5px 5px 0px; -webkit-border-radius:0px 5px 5px 0px; border-radius:0px 5px 5px 0px; }


#feedArea .content .video_info { margin:0; width:600px; height:30px; float:left; }
#feedArea .content .video_info .video_title { float:left; width:300px; height:30px; line-height:30px; color:#333; font-size:14px; font-weight:700; overflow:hidden; }
#feedArea .content .video_info .video_cover { float:right; width:200px; height:30px; line-height:30px; color:#999; }
#feedArea .content .video_info .video_cover a { color:#999; font-size:12px; float:right; margin:0 0 0 10px; cursor:pointer; }
#feedArea .content .video_info .video_cover a:hover { text-decoration:underline; }
